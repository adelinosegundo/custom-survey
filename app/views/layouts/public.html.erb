<!doctype html>
<html lang="pt-BR">
<head>
    <meta charset="UTF-8">
    <title>Custom Survey<%= " - #{page_title}" rescue '' %></title>
    <link rel="pingback" href="">
    <link rel="shortcut icon" href="/assets/images/favicon.png">
    <link rel="dns-prefetch" href="http://www.google-analytics.com">
    <meta name="robots" content="index,follow">
    <meta name="viewport" content="initial-scale=1.0 maximum-scale=1.0 user-scalable=no">

    <!-- Metatags -->
    <meta name="author" content="">
    <meta name="description" content="">
    <meta name="keywords" content="">

    <!-- Facebook Metatags -->
    <meta property="fb:app_id" content=""/>
    <meta property="article:author" content=""/>
    <meta property="article:publisher" content=""/>
    <meta property="article:published_time" content=""/>
    <meta property="og:image" content=""/>
    <meta property="og:url" content=""/>
    <meta property="og:title" content=""/>
    <meta property="og:type" content=""/>
    <meta property="og:description" content=""/>
    <meta property="og:site_name" content=""/>

    <!-- Normalize.css -->
    <link rel="stylesheet" href="/bower_components/normalize-css/normalize.css">

    <!-- Animate.css -->
    <link rel="stylesheet" href="/bower_components/animate.css/animate.css">

    <!-- Sweetalert -->
    <link rel="stylesheet" href="/bower_components/sweetalert2/dist/sweetalert2.css">

    <!-- Main Style -->
    <link rel="stylesheet" href="/assets/styles/main.css">

    <!-- Awesome Bootstrap Checkbox -->
    <link href="/bower_components/awesome-bootstrap-checkbox/awesome-bootstrap-checkbox.css" rel="stylesheet">

    <!-- Main Font -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:400,600,700|Raleway:400,500,600,700" rel="stylesheet">

    <!-- Font Awesome -->
    <link href="/bower_components/font-awesome/css/font-awesome.min.css" rel="stylesheet">

    <%= csrf_meta_tags %>
</head>
<body>

    <div class="page-wrapper">
        <%= yield %>
    </div>

    <!-- Jquery -->
    <script src="/bower_components/jquery/dist/jquery.min.js"></script>

    <!-- Bootstrap Scripts -->
    <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap.min.js"></script>

    <!-- Select2 -->
    <script src="/bower_components/select2/dist/js/select2.min.js"></script>
    <script src="/bower_components/select2/dist/js/i18n/pt-BR.js"></script>
    <link href="/bower_components/select2/dist/css/select2.min.css" rel="stylesheet">
    <link href="/bower_components/select2-bootstrap-theme/dist/select2-bootstrap.min.css" rel="stylesheet">

    <!-- Sweetalert -->
    <script src="/bower_components/sweetalert2/dist/sweetalert2.min.js"></script>

    <!-- Jquery Validate -->
    <script src="/bower_components/jquery-validation/dist/jquery.validate.js"></script>

    <!-- Jquery Mask -->
    <script src="/bower_components/jquery-mask-plugin/dist/jquery.mask.min.js"></script>

    <!-- Scroll Magic -->
    <script src="//cdnjs.cloudflare.com/ajax/libs/ScrollMagic/2.0.5/ScrollMagic.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/ScrollMagic/2.0.5/plugins/debug.addIndicators.min.js"></script>

    <!-- Jquery Bar Rating -->
    <script src="/bower_components/jquery-bar-rating/dist/jquery.barrating.min.js"></script>
    <link rel="stylesheet" href="/bower_components/jquery-bar-rating/dist/themes/fontawesome-stars.css">

    <!-- Application Script -->
    <%= javascript_include_tag 'application' %>

    <!-- Main Script-->
    <script src="/assets/scripts/main.min.js"></script>

    <!-- Page Specific Scripts -->
    <%= yield :javascript %>

    <!-- Alert Script -->
    <% if notice %>
    <% status = flash[:status] ? flash[:status] : "success" %>
    <script>
        $(document).ready(function() {
            swal({
                title: "<%= status == "success" ? "Pronto!" : "Ops!" %>", text: "<%= notice %>", type: "<%= status %>",
                confirmButtonColor: "#227DBF", customClass: "sweet-alert--custom"
            });
        })
    </script>
    <% end %>

    <!-- Google Analytics -->
    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-100150746-1', 'auto');
        ga('send', 'pageview');
    </script>
</body>
</html>
